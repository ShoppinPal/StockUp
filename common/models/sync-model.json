{
  "name": "SyncModel",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Utils": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "syncInProcess": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "version": {
      "type": "number",
      "required": true
    },
    "lastSyncedAt": {
      "type": "date"
    },
    "workerTaskId": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "orgModel": {
      "type": "belongsTo",
      "model": "OrgModel",
      "foreignKey": "orgModelId"
    }
  },
  "acls": [],
  "methods": {}
}
