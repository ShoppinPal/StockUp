{
  "name": "UserModel",
  "base": "User",
  "idInjection": true,
  "mixins": {
    "Timestamp": {},
    "Context": {}
  },
  "injectOptionsFromRemoteContext": true,
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "generated": true
    },
    "emailVerified": {
      "type": "boolean",
      "default": false
    },
    "virtual": {
      "type": "boolean",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "teamModels": {
      "type": "hasMany",
      "model": "TeamModel",
      "foreignKey": "ownerId"
    },
    "globalConfigModels": {
      "type": "hasOne",
      "model": "GlobalConfigModel",
      "foreignKey": "userModelToGlobalConfigModelId"
    },
    "storeConfigModel": {
      "type": "belongsTo",
      "model": "StoreConfigModel",
      "foreignKey": "storeConfigModelId"
    },
    "supplierModels": {
      "type": "hasMany",
      "model": "SupplierModel",
      "foreignKey": "userId"
    },
    "storeModels": {
      "type": "hasMany",
      "model": "StoreModel",
      "foreignKey": "userModelId",
      "through": "UserStoreMapping"
    },
    "reportModels": {
      "type": "hasMany",
      "model": "ReportModel",
      "foreignKey": "userModelId"
    },
    "stockOrderLineitemModels": {
      "type": "hasMany",
      "model": "StockOrderLineitemModel",
      "foreignKey": "userId"
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    },
    "orgModel": {
      "type": "belongsTo",
      "model": "OrgModel",
      "foreignKey": ""
    },
    "commentModels": {
      "type": "hasMany",
      "model": "CommentModel",
      "foreignKey": "userModelId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "profile"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "signup"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__orgModel"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "setPassword"
    }
  ],
  "methods": {}
}
