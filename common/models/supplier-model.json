{
    "name": "SupplierModel",
    "base": "PersistedModel",
    "idInjection": true,
    "mixins": {
        "Utils": true
    },
    "properties": {
        "name": {
            "type": "string",
            "required": true
        },
        "apiId": {
            "type": "string",
            "required": true
        }
    },
    "validations": [],
    "relations": {
        "userModel": {
            "type": "belongsTo",
            "model": "UserModel",
            "foreignKey": "userId"
        },
        "storeConfigModel": {
            "type": "belongsTo",
            "model": "StoreConfigModel",
            "foreignKey": "storeConfigModelToSupplierModelId"
        },
        "productModels": {
            "type": "hasMany",
            "model": "ProductModel",
            "foreignKey": "productModelId"
        },
        "orgModel": {
            "type": "belongsTo",
            "model": "OrgModel",
            "foreignKey": ""
        }
    },
    "acls": [
        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        },
        {
            "accessType": "READ",
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW"
        },
        {
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW",
            "property": "create"
        },
        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "$owner",
            "permission": "ALLOW"
        },
        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "admin",
            "permission": "ALLOW"
        }
    ],
    "methods": {}
}
