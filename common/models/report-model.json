{
  "name": "ReportModel",
  "base": "PersistedModel",
  "idInjection": true,
  "mixins": {
    "Utils": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "generated": true
    },
    "name": {
      "type": "string",
      "required": false
    },
    "created": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "state": {
      "type": "string",
      "required": true
    },
    "supplier": {
      "type": "object",
      "required": false
    },
    "outlet": {
      "type": "object",
      "required": false
    },
    "totalRows": {
      "type": "number",
      "required": false
    },
    "workerTaskId": {
      "type": "string",
      "required": false
    },
    "workerStatus": {
      "type": "string",
      "required": false
    },
    "vendConsignmentId": {
      "type": "string",
      "required": false
    },
    "vendConsignment": {
      "type": "object",
      "required": false
    },
    "importedFromFile": {
      "type": "boolean"
    },
    "discrepancies": {
      "type": "number",
      "required": false
    }
  },
  "validations": [],
  "relations": {
