      "foreignKey": "reportModelId"
    },
    "storeConfigModel": {
      "type": "belongsTo",
      "model": "StoreConfigModel",
      "foreignKey": "storeConfigModelId"
    },
    "orgModel": {
      "type": "belongsTo",
      "model": "OrgModel",
      "foreignKey": ""
