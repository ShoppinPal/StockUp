    "storeModel": {
      "type": "belongsTo",
      "model": "StoreModel",
      "foreignKey": "storeModelId"
    },
    "categoryModel": {
      "type": "belongsTo",
      "model": "CategoryModel",
      "foreignKey": "categoryModelId"
    },
    "supplierModel": {
      "type": "belongsTo",
      "model": "SupplierModel",
      "foreignKey": "supplierModelId"
    },
    "deliverFromStoreModel": {
      "type": "belongsTo",
      "model": "StoreModel",
      "foreignKey": "deliverFromStoreModelId"
