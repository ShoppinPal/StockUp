    "orderConfigModel": {
      "type": "belongsTo",
      "model": "OrderConfigModel",
      "foreignKey": "orderConfigModelId"
    },
    "approvedByUserModel": {
      "type": "belongsTo",
      "model": "UserModel",
      "foreignKey": "approvedByUserModelId"
    },
    "fulfilledByUserModel": {
      "type": "belongsTo",
      "model": "UserModel",
      "foreignKey": "fulfilledByUserModelId"
    },
    "receivedByUserModel": {
      "type": "belongsTo",
      "model": "UserModel",
      "foreignKey": "receivedByUserModelId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
