{
    "initial:before": {
        "loopback#favicon": {},
        "loopback#context": {},
        "loopback#token": {
            "params": [
                "state"
            ]
        },
        "./middleware/userContext": {}
    },
    "initial": {
        "compression": {}
    },
    "session": {},
    "auth": {},
    "parse": {},
    "routes:before": {
        "./middleware/accessLogger": {
            "paths": "/api/*"
        }
    },
    "files": {
        "loopback#static": [
            {
                "params": "$!../.tmp"
            },
            {
                "params": "$!../client/app"
            },
            {
                "params": "$!../client/admin"
            }
        ]
    },
    "final": {
        "loopback#urlNotFound": {}
    },
    "final:after": {
        "loopback#errorHandler": {}
    }
}
