{
  "initial:before": {
    "loopback#favicon": {},
    "loopback#context": {},
    "loopback#token": {
      "params": [
        "state"
      ]
    }
  },
  "initial": {
    "compression": {
      "enabled": false
    }
  },
  "session": {},
  "auth": {},
  "parse": {
    "body-parser#raw": {
      "params": {
        "limit": "50mb"
      }
    }
  },
  "routes:before": {
    "./middleware/accessLogger": {
      "paths": "/api/*"
    }
  },
  "files": {
    "loopback#static": [
      {
        "params": "$!../.tmp"
      },
      {
        "params": "$!../client/app"
      },
      {
        "params": "$!../client/admin"
      }
    ]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "loopback#errorHandler": {}
  }
}
